<?xml version = "1.0" encoding = "UTF-8" ?>
<Application xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:local="clr-namespace:Proyecto_App_AlmaFria"
             xmlns:convert="clr-namespace:Proyecto_App_AlmaFria.Convert"
             x:Class="Proyecto_App_AlmaFria.App">
    <Application.Resources>
        <ResourceDictionary>
            <convert:ConvertUpper x:Key="convertUpper" />
            <convert:StringNotEmptyConverter x:Key="stringNotEmptyConverter" />
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Resources/Styles/Colors.xaml" />
                <ResourceDictionary Source="Resources/Styles/Styles.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
